            <kind>1</kind>
            <string>//PHYSICS
move_y += 0.4
//reset on floor
if place_meeting(x,y+1,obj_solid)
{move_y = 0;}
//ceiling bump
if place_meeting(x,y-1,obj_solid)
and move_y &lt; 0
{move_y = 0;}

//STATES
_on_ground = false
if place_meeting(x,y+1,obj_solid){_on_ground = true}
if move_x &gt; 0 {_facing_right = true}
if move_x &lt; 0 {_facing_right = false}

//CONTROLS
//move
if keyboard_check(vk_left){move_x -= 1}
if keyboard_check(vk_right){move_x += 1}
//jump
if keyboard_check(ord('Z')) and _on_ground{move_y -= 5.5;}
//slash
if keyboard_check(ord('X')){_slashing = true}
</string>
