            <string>event_inherited()

//BEGIN STEP
_slashing = false
_hovering = false
_facing_right = false
_on_ground = false

//DRAW
_sprite = moth_idle
_frame = 0
_animspeed = 0.3
__old_sprite = _sprite

_hover_sprite = moth_jump_hover
_hover_frame = 0
_hover_animspeed = 0.5
</string>
